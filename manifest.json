{
  "manifest_version": 3,
  "name": "WDYWFM",
  "version": "1.0.0",
  "description": "Straightforward network request analysis with session tracking and bot detection detection",
  "permissions": [
    "storage",
    "webRequest",
    "cookies"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
